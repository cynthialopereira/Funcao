
<form id="formBeneficiario" method="post">

    <div class="row">
        <div class="col-lg-12">
            <div class="pull-left">
                <a href="#" class="btn btn-default" data-toggle="modal" data-target="#myModal">Beneficiários</a>
                <div class="modal fade" id="myModal">
                    <div class="modal-dialog">
                        <div class="modal-content">

                            <div class="modal-header">
                                <a href="#" class="close" data-dismiss="modal">&times;</a>
                                <h3 class="modal-title">Beneficiários</h3>
                            </div>
                            <div class="modal-body">
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label for="CPF">CPF:</label>
                                            <input type="text" class="form-control" id="CpfBeneficiario" name="CpfBeneficiario" placeholder="Ex.: 999.999.999-99" maxlength="14">
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label for="Nacionalidade">Nome:</label>
                                            <input type="text" class="form-control" id="NomeBeneficiario" name="NomeBeneficiario" placeholder="Ex.: João" maxlength="50">
                                        </div>
                                    </div>
                                    <div class="col-md-2">
                                        <div class="form-group">
                                            <label>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</label>
                                            <button type="button" class="btn btn-sm btn-success" onclick="InserirBeneficiarioJS();">Incluir</button>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-12" id="Mensagem">

                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-12">
                                        <table id="gridBeneficiarios" class="table"></table>
                                    </div>
                                </div>
                            </div>
                            <div class="modal-footer">
                                <a href="#" class="btn btn-default" data-dismiss="modal">Fechar</a>

                            </div>
                        </div>

                    </div>


                </div>
            </div>
        </div>

    </div>


</form>

